class BankAccount:
    def __init__(self, account_holder, account_number, balance=0):
        self.account_holder = account_holder
        self.account_number = account_number
        self.balance = balance
        self.history = []

    def deposit(self, amount):
        self.balance += amount
        self.history.append(f"Deposited: ${amount}")
        print(f"Deposited ${amount} to {self.account_holder}'s account.")

    def withdraw(self, amount):
        if amount > self.balance:
            print(f"Error: Insufficient balance for {self.account_holder}!")
        else:
            self.balance -= amount
            self.history.append(f"Withdrew: ${amount}")
            print(f"Withdrew ${amount} from {self.account_holder}'s account.")

    def get_balance(self):
        print(f"Current Balance for {self.account_holder}: ${self.balance}")

    def transaction_history(self):
        print(f"--- Transaction History for {self.account_number} ---")
        for transaction in self.history:
            print(transaction)

acc1 = BankAccount("Jitu", "101")
acc1.deposit(1000)
acc1.withdraw(200)
acc1.withdraw(500)
acc1.get_balance()
acc1.transaction_history()

acc2 = BankAccount("Itadori", "102")
acc2.deposit(500)
acc2.withdraw(600)
acc2.deposit(200)
acc2.get_balance()
acc2.transaction_history()
